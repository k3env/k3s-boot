write-kubeconfig-mode: "0644"
cluster-cidr: "{{ k3s_cluster_cidr }}"
service-cidr: "{{ k3s_service_cidr }}"
service-node-port-range: "1-65535"
node-external-ip: "{{ ansible_facts.default_ipv4.address }}"
disable:
  - servicelb
  - traefik
  - local-storage
  - metrics-server
docker: true
datastore-endpoint: "{{ datastore }}"
server: "https://{{ k3s_cluster_endpoint }}:6443"
token: "{{ k3s_token }}"
